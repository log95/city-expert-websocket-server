{
    "name": "log95/city-expert-websocket-server",
    "description": "City expert websocket server",
    "type": "project",
    "require": {
        "firebase/php-jwt": "^5.2",
        "php": "^7.4",
        "ext-json": "*",
        "cboden/ratchet": "^0.4.3",
        "react/zmq": "^0.4.0",
        "vlucas/phpdotenv": "^5.2",
        "ext-zmq": "*"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    }
}
